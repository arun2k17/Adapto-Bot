{
  "type": "AdaptiveCard",
  "$schema": "https://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.5",
  "body": [
    {
      "type": "TextBlock",
      "text": "New Icon element",
      "wrap": true,
      "size": "Large"
    },
    {
      "type": "Icon",
      "name": "Calendar",
      "size": "Large",
      "style": "Regular"
    },
    {
      "type": "TextBlock",
      "text": "Clickable icons",
      "wrap": true,
      "size": "Large",
      "separator": true,
      "spacing": "large"
    },
    {
      "type": "Icon",
      "name": "Emoji",
      "selectAction": {
        "type": "Action.OpenUrl",
        "url": "https://www.microsoft.com",
        "title": "Click me!",
        "tooltip": "Icons can handle actions"
      }
    },
    {
      "type": "TextBlock",
      "text": "Icons on buttons",
      "wrap": true,
      "size": "Large",
      "separator": true,
      "spacing": "large"
    }
  ],
  "actions": [
    {
      "type": "Action.OpenUrl",
      "title": "Regular icon",
      "url": "https://www.microsoft.com",
      "iconUrl": "icon:AccessTime"
    },
    {
      "type": "Action.OpenUrl",
      "title": "Filled icon",
      "url": "https://www.microsoft.com",
      "iconUrl": "icon:AccessTime,filled"
    }
  ]
}
